                                            "    var report = 'reportSchetVik';",
                                            "    var namereport = 'Распечатка счетов';",
                                            "    var value = {",
                                            "        login:values.get('login'),",
                                            "        password:values.get('password'),",
                                            "        report:report,",
                                            "        what:report,",
                                            "        date_from:data_from,",
                                            "        date_to:data_to",
                                            "    };",
                                            "    var tab = tabPnCenter.child('#'+report);",
                                            "    if (!tab) {",
                                            "        tab  = tabPnCenter.add({",
                                            "            xtype:'tabreportorg',",
                                            "            title:namereport,",
                                            "            id:''+report+''",
                                            "        });",
                                            "",
